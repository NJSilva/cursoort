Create or Replace Procedure ESQUEMA.SP_Prestamos_Update(
IN P_LIBROS_ID DECIMAL(5,0),
IN P_PERSONAS_ID DECIMAL(5,0),
IN P_PRESTAMOS_FECHA_DESDE DATE,
IN P_PRESTAMOS_FECHA_HASTA DATE
)
P1:Begin

Update ESQUEMA.PRESTAMOS SET 
LIBROS_ID=P_LIBROS_ID,
PERSONAS_ID=P_PERSONAS_ID,
PRESTAMOS_FECHA_DESDE=P_PRESTAMOS_FECHA_DESDE,
PRESTAMOS_FECHA_HASTA=P_PRESTAMOS_FECHA_HASTA
WHERE
LIBROS_ID=P_LIBROS_ID
AND PERSONAS_ID=P_PERSONAS_ID
;

End P1