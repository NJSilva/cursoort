Create or Replace Procedure ESQUEMA.SP_Login_ObtenerPK(
P_PERSONAS_CEDULA VARCHAR(10)
)
DYNAMIC RESULT SETS 1

P1:Begin

DECLARE c1 CURSOR WITH RETURN FOR Select 
LOGIN.LOGIN_CLAVE,
LOGIN.PERSONAS_ID,
PERSONAS.PERSONAS_CEDULA,
PERSONAS.PERSONAS_MAIL,
PERSONAS.PERSONAS_NOMBRE
from 
ESQUEMA.LOGIN LOGIN, ESQUEMA.PERSONAS PERSONAS 
WHERE login.PERSONAS_ID = personas.PERSONAS_ID
AND PERSONAS.PERSONAS_CEDULA=P_PERSONAS_CEDULA
;

OPEN C1;

End p1