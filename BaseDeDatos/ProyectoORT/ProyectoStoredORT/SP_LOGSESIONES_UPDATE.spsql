CREATE OR REPLACE PROCEDURE ESQUEMA.SP_LOGSESIONES_UPDATE
(
IN @PERSONAS_ID NUMERIC(5,0),
IN @INGRESO TIMESTAMP,
IN @FIN TIMESTAMP,
IN @TOKEN VARCHAR(30)
)

P1: BEGIN

	UPDATE ESQUEMA.LOGSESIONES
	SET 
		FIN = @FIN
	WHERE
		PERSONAS_ID=@PERSONAS_ID AND
		INGRESO=@INGRESO AND
		TOKEN=@TOKEN
	;
		
	

END P1