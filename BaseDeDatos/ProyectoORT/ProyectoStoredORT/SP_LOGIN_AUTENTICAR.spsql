Create or Replace Procedure ESQUEMA.SP_LOGIN_AUTENTICAR(
P_PERSONAS_CEDULA VARCHAR(10),
P_LOGIN_CLAVE VARCHAR(10)
)
DYNAMIC RESULT SETS 1

P1:Begin

DECLARE c1 CURSOR WITH RETURN FOR 
Select 
	PERSONAS.PERSONAS_CEDULA,
	PERSONAS.PERSONAS_MAIL,
	PERSONAS.PERSONAS_NOMBRE
from 
	ESQUEMA.LOGIN LOGIN, 
	ESQUEMA.PERSONAS PERSONAS 
WHERE 
PERSONAS_CEDULA = P_PERSONAS_CEDULA
AND LOGIN_CLAVE = P_LOGIN_CLAVE
;

OPEN C1;

End p1