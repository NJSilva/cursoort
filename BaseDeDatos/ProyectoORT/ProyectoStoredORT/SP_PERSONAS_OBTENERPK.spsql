Create or Replace Procedure ESQUEMA.SP_Personas_ObtenerPK(
IN P_PERSONAS_ID DECIMAL(5,0))
DYNAMIC RESULT SETS 1

P1:Begin

DECLARE c1 CURSOR WITH RETURN FOR Select 
PERSONAS.PERSONAS_ID,
PERSONAS.PERSONAS_CEDULA,
PERSONAS.PERSONAS_MAIL,
PERSONAS.PERSONAS_NOMBRE
from ESQUEMA.PERSONAS PERSONAS WHERE 
PERSONAS.PERSONAS_ID=P_PERSONAS_ID ;
OPEN C1;

End p1