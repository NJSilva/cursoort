Create or Replace Procedure ESQUEMA.SP_Personas_Update(
IN P_PERSONAS_ID DECIMAL(5,0),
IN P_PERSONAS_CEDULA VARCHAR(10),
IN P_PERSONAS_MAIL VARCHAR(50),
IN P_PERSONAS_NOMBRE VARCHAR(120)
)
P1:Begin

Update ESQUEMA.PERSONAS SET 
PERSONAS_MAIL=P_PERSONAS_MAIL,
PERSONAS_NOMBRE=P_PERSONAS_NOMBRE

 WHERE 
PERSONAS_ID=P_PERSONAS_ID 
;

End P1