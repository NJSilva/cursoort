Create or Replace Procedure ESQUEMA.SP_Reservas_Update(
IN @PERSONAS_CEDULA VARCHAR(10),
IN @LIBROS_CODIGO VARCHAR(12)
)
P1:Begin

Update ESQUEMA.RESERVAS SET 
PERSONAS_CEDULA=@PERSONAS_CEDULA,
LIBROS_CODIGO=@LIBROS_CODIGO
WHERE
PERSONAS_CEDULA=@PERSONAS_CEDULA AND LIBROS_CODIGO=@LIBROS_CODIGO
;

End P1