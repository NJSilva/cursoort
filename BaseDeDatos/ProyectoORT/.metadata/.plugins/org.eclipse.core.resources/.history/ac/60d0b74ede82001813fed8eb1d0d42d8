Create or Replace Procedure ESQUEMA.SP_Prestamos_Update(
IN @LIBROS_CODIGO VARCHAR(12),
IN @PERSONAS_CEDULA VARCHAR(10),
IN @PRESTAMOS_FECHA_DESDE DATE
)
P1:Begin

Update ESQUEMA.PRESTAMOS SET 
LIBROS_CODIGO=@LIBROS_CODIGO,
PERSONAS_CEDULA=@PERSONAS_CEDULA,
PRESTAMOS_FECHA_DESDE=@PRESTAMOS_FECHA_DESDE,
PRESTAMOS_FECHA_HASTA=@PRESTAMOS_FECHA_HASTA

 WH
;

End P1