Create or Replace Procedure ESQUEMA.SP_Reservas_ObtenerPK(
IN P_LIBROS_CODIGO VARCHAR(12),
IN P_PERSONAS_CEDULA VARCHAR(10)
DYNAMIC RESULT SETS 1

P1:Begin

DECLARE c1 CURSOR WITH RETURN FOR Select 
RESERVAS.PERSONAS_CEDULA,
RESERVAS.LIBROS_CODIGO,
PERSONAS.PERSONAS_CEDULA,
PERSONAS.PERSONAS_MAIL,
PERSONAS.PERSONAS_NOMBRE,
LIBROS.LIBROS_CODIGO,
LIBROS.LIBROS_TITULO,
LIBROS.LIBROS_ISBN,
LIBROS.LIBROS_ANIO,
LIBROS.LIBROS_DESCRIPCION,
LIBROS.LIBROS_DISPONIBLE,
LIBROS.TIPOS_CODIGO
from 
	ESQUEMA.RESERVAS RESERVAS, 
	ESQUEMA.PERSONAS PERSONAS, 
	ESQUEMA.LIBROS LIBROS 
WHERE
	reservas.PERSONAS_CEDULA = personas.PERSONAS_CEDULA 
	and  reservas.LIBROS_CODIGO = libros.LIBROS_CODIGO
	
	AND RESERVAS.LIBROS_CODIGO=P_LIBROS_CODIGO
	AND RESERVAS.PERSONAS_CEDULA=P_PERSONAS_CEDULA
	
	;
OPEN C1;

End p1