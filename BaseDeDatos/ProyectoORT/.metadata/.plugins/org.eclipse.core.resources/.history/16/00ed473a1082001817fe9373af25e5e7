Create or Replace Procedure ESQUEMA.SP_Prestamos_Insert(
--Campos de la tabla 
IN @LIBROS_CODIGO VARCHAR(12),
IN @PERSONAS_CEDULA VARCHAR(10),
IN @PRESTAMOS_FECHA_DESDE DATE,
IN @PRESTAMOS_FECHA_HASTA DATE,
IN @LIBROS_CODIGO VARCHAR(12),
IN @PERSONAS_CEDULA VARCHAR(10)
)
P1:Begin

Insert into ESQUEMA.PRESTAMOS(
LIBROS_CODIGO,
PERSONAS_CEDULA,
PRESTAMOS_FECHA_DESDE,
PRESTAMOS_FECHA_HASTA,
LIBROS_CODIGO,
PERSONAS_CEDULA
) values (
@LIBROS_CODIGO,
@PERSONAS_CEDULA,
@PRESTAMOS_FECHA_DESDE,
@PRESTAMOS_FECHA_HASTA,
@LIBROS_CODIGO,
@PERSONAS_CEDULA
);

End P1