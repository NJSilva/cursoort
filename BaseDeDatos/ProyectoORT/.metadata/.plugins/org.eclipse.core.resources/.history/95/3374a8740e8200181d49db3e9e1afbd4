Create or Replace Procedure ESQUEMA.SP_Login_Obtener()

DYNAMIC RESULT SETS 1

P1:Begin

DECLARE c1 CURSOR WITH RETURN FOR Select 
LOGIN.LOGIN_CLAVE,
LOGIN.PERSONAS_CEDULA,
PERSONAS.PERSONAS_CEDULA,
PERSONAS.PERSONAS_MAIL,
PERSONAS.PERSONAS_NOMBRE
from ESQUEMA.LOGIN LOGIN
, ESQUEMA.PERSONAS PERSONAS
 where  login.PERSONAS_CEDULA = personas.PERSONAS_CEDULA
;
OPEN C1;


End p1@