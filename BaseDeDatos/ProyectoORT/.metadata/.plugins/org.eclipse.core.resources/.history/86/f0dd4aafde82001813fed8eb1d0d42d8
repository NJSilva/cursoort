Create or Replace Procedure ESQUEMA.SP_Personas_Update(
IN @PERSONAS_CEDULA VARCHAR(10),
IN @PERSONAS_MAIL VARCHAR(50),
IN @PERSONAS_NOMBRE VARCHAR(120)
)
P1:Begin

Update ESQUEMA.PERSONAS SET 
PERSONAS_MAIL=@PERSONAS_MAIL,
PERSONAS_NOMBRE=@PERSONAS_NOMBRE

 WHERE 
PERSONAS_CEDULA=@PERSONAS_CEDULA 
;

End P1