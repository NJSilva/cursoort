Create or Replace Procedure ESQUEMA.SP_Reservas_Update(
IN P_PERSONAS_CEDULA VARCHAR(10),
IN P_LIBROS_CODIGO VARCHAR(12)
)
P1:Begin

Update ESQUEMA.RESERVAS SET 
PERSONAS_CEDULA=P_PERSONAS_CEDULA,
LIBROS_CODIGO=P_LIBROS_CODIGO
WHERE
PERSONAS_CEDULA=P_PERSONAS_CEDULA AND LIBROS_CODIGO=P_LIBROS_CODIGO
;

End P1