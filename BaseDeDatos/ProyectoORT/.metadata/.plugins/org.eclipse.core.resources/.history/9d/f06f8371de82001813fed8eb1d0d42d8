Create or Replace Procedure ESQUEMA.SP_Libros_Update(
IN @LIBROS_CODIGO VARCHAR(12),
IN @LIBROS_TITULO VARCHAR(150),
IN @LIBROS_ISBN VARCHAR(25),
IN @LIBROS_ANIO DECIMAL(4,0),
IN @LIBROS_DESCRIPCION VARCHAR(300),
IN @LIBROS_DISPONIBLE DECIMAL(5,0),
IN @TIPOS_CODIGO DECIMAL(5,0),
IN @TIPOS_CODIGO DECIMAL(5,0)
)
P1:Begin

Update ESQUEMA.LIBROS SET 
LIBROS_TITULO=@LIBROS_TITULO,
LIBROS_ISBN=@LIBROS_ISBN,
LIBROS_ANIO=@LIBROS_ANIO,
LIBROS_DESCRIPCION=@LIBROS_DESCRIPCION,
LIBROS_DISPONIBLE=@LIBROS_DISPONIBLE,
TIPOS_CODIGO=@TIPOS_CODIGO

 WHERE 
LIBROS_CODIGO=@LIBROS_CODIGO 
;

End P1