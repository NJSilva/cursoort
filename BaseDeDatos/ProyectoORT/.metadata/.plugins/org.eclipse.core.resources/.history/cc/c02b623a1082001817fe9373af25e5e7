Create or Replace Procedure ESQUEMA.SP_Libros_Obtener()

DYNAMIC RESULT SETS 1

P1:Begin

DECLARE c1 CURSOR WITH RETURN FOR Select 
LIBROS.LIBROS_CODIGO,
LIBROS.LIBROS_TITULO,
LIBROS.LIBROS_ISBN,
LIBROS.LIBROS_ANIO,
LIBROS.LIBROS_DESCRIPCION,
LIBROS.LIBROS_DISPONIBLE,
LIBROS.TIPOS_CODIGO,
TIPOS.TIPOS_NOMBRE
from ESQUEMA.LIBROS LIBROS
, ESQUEMA.TIPOS TIPOS
 where  libros.TIPOS_CODIGO = tipos.TIPOS_CODIGO
;
OPEN C1;


End p1